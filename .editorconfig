# EditorConfig: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
indent_style = tab
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.{json,json.exemple,md}]
indent_style = space

[*.{ps1,psm1,psd1,ps1xml,pssc,psrc,cdxml}]
# PowerShell doit Ãªtre en UTF-8 avec BOM
charset = utf-8-bom
